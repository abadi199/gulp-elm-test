  },
  "homepage": "https://github.com/abadi199/gulp-elm-test#readme",
  "dependencies": {
    "cross-spawn-async": "^2.1.9",
    "gulp-util": "^3.0.7",
    "minimist": "^1.2.0",
    "q": "^1.4.1",
    "through2": "^2.0.1",
    "elm-test": "^0.16.1-alpha3"
  },
